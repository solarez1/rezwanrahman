DEVICE=eth0
BOOTPROTO=dhcp
ONBOOT=yes
TYPE=Ethernet
USERCTL=yes
PEERDNS=off
IPV6INIT=no
PERSISTENT_DHCLIENT=yes
RES_OPTIONS="timeout:2 attempts:5"
DHCP_ARP_CHECK=no
DOMAIN="<%= @aws['ad_domain'] %> <%= @aws['region'] %>.compute.internal"
DNS1=<%= (@aws['ad_servers_map'])[@aws['region']][:primary_dc_ip] %>